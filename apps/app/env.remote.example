# Supabase Remote Configuration
USE_SUPABASE_LOCAL=false
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_remote_anon_key
SUPABASE_SERVICE_KEY=your_remote_service_key
SUPABASE_PROJECT_ID=your_project_id

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_SECRET=your_google_secret
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_SECRET=your_discord_secret

# Database (Neon)
USE_DRIZZLE=true
DATABASE_URL=your_neon_database_url

# Email Service
RESEND_API_KEY=your_resend_api_key
SEND_EMAIL_HOOK_SECRET=your_webhook_secret

# Redis (Upstash)
UPSTASH_REDIS_REST_URL=your_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# Analytics (OpenPanel)
OPENPANEL_SECRET_KEY=your_openpanel_key
NEXT_PUBLIC_OPENPANEL_CLIENT_ID=your_openpanel_client_id

# App Configuration
NEXT_PUBLIC_APP_URL=https://your-domain.com
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Trigger.dev (Background Jobs)
TRIGGER_API_KEY=your_trigger_api_key
TRIGGER_API_URL=https://api.trigger.dev

# Sentry (Error Tracking)
SENTRY_AUTH_TOKEN=your_sentry_auth_token
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project

# Cloudflare R2 (Storage)
R2_ACCOUNT_ID=your_r2_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your_r2_bucket_name
