{
  "name": "@v1/database",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "biome check .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "generate": "drizzle-kit generate:pg",
    "migrate": "drizzle-kit up:pg",
    "push": "drizzle-kit up:pg",
    "studio": "drizzle-kit studio",
    "migrate-data": "bun run src/migrate-data.ts",
    "provision-neon": "bun run src/provision-neon.ts",
    "setup-complete": "bun run src/setup-complete.ts",
    "seed": "bun run src/seed.ts run --force",
    "seed:run": "bun run src/seed.ts run",
    "seed:list": "bun run src/seed.ts list",
    "seed:help": "bun run src/seed.ts help"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@neondatabase/serverless": "^0.9.0",
    "@v1/logger": "workspace:*",
    "drizzle-orm": "^0.29.0"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.0",
    "typescript": "^5.5.4"
  },
  "exports": {
    "./drizzle": "./src/drizzle.ts",
    "./schema": "./src/schema/index.ts",
    "./queries": "./src/queries/index.ts",
    "./mutations": "./src/mutations/index.ts",
    "./types": "./src/types.ts",
    "./adapters": "./src/adapters/database-adapter.ts",
    "./seeders": "./src/seeders/index.ts"
  }
}
