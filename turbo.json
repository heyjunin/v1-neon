{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "generate": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "push": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "seed": {
      "cache": false
    },
    "seed:run": {
      "cache": false
    },
    "seed:list": {
      "cache": false
    },
    "seed:help": {
      "cache": false
    },
    "migrate-data": {
      "cache": false
    },
    "provision-neon": {
      "cache": false
    },
    "setup-complete": {
      "cache": false
    }
  }
}
